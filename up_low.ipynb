    "def up_low(s):\n",
    "  up=0\n",
    "  low=0\n",
    "  for i in s:\n",
    "    if i.isupper():\n",
    "      up+=1\n",
    "    else:\n",
    "      low+=1\n",
    "  print'Number of uppercase characters:', up\n",
    "  print'Number of lowercase characters:', low\n",
    "        \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
